{
  "name": "censuschat-backend",
  "version": "0.1.0",
  "description": "CensusChat backend API - Natural language interface for US Census data",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "seed:demo": "ts-node src/scripts/seedDemoData.ts",
    "test:export": "ts-node src/scripts/testExport.ts",
    "load-acs-data": "ts-node scripts/load-acs-data.ts",
    "load-blockgroups": "ts-node scripts/load-acs-blockgroup.ts",
    "load-blockgroups-expanded": "ts-node scripts/load-acs-blockgroup-expanded.ts",
    "load-tracts": "ts-node scripts/load-acs-tract.ts",
    "load-states": "ts-node scripts/load-acs-state.ts",
    "create-geo-hierarchy": "ts-node scripts/create-geo-hierarchy.ts",
    "load-all-geography": "npm run load-states && npm run load-tracts && npm run load-blockgroups-expanded && npm run create-geo-hierarchy",
    "test-blockgroup": "ts-node scripts/test-blockgroup-load.ts",
    "duckdb": "duckdb data/census.duckdb",
    "eval": "ts-node src/evals/query-eval.ts",
    "secret-scan": "git diff --cached --name-only -z | xargs -0 grep -l -E '(sk-ant-api03-[a-zA-Z0-9_-]{95}|[a-f0-9]{40})' 2>/dev/null && echo '\n❌ ERROR: Potential API key detected! Remove before committing.\n' && exit 1 || exit 0",
    "prepare": "husky"
  },
  "keywords": [
    "census",
    "api",
    "natural-language",
    "demographics",
    "duckdb"
  ],
  "author": "CensusChat Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.64.0",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@types/stream-buffers": "^3.0.7",
    "@types/uuid": "^11.0.0",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "duckdb": "^1.3.2",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "node-sql-parser": "^5.3.7",
    "pg": "^8.16.3",
    "redis": "^5.7.0",
    "stream-buffers": "^3.0.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.1.0",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
